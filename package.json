{
  "name": "reverse-image-finder",
  "version": "0.1.0",
  "description": "A reverse image finder application.",
  "main": "src/start.ts",
  "scripts": {
    "start": "electron .",
    "pack": "electron-builder --dir",
    "dist": "electron-builder",
    "publish": "electron-builder --publish always"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Glinkis/reverse-image-finder.git"
  },
  "author": "Victor Glind√•s",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/Glinkis/reverse-image-finder/issues"
  },
  "homepage": "https://github.com/Glinkis/reverse-image-finder#readme",
  "dependencies": {
    "electron-compile": "6.4.2",
    "electron-devtools-installer": "2.2.4",
    "electron-squirrel-startup": "1.0.0",
    "electron-updater": "^2.21.10",
    "jpeg-js": "0.3.4",
    "mobx": "5.0.3",
    "mobx-react": "5.2.3",
    "pixelmatch": "4.0.2",
    "pngjs": "3.3.3",
    "psd": "3.2.0",
    "react": "16.4.1",
    "react-dom": "16.4.1",
    "react-hot-loader": "4.3.3"
  },
  "devDependencies": {
    "@types/electron": "1.6.10",
    "@types/electron-devtools-installer": "2.0.3",
    "@types/pixelmatch": "4.0.0",
    "@types/react": "16.4.1",
    "@types/react-dom": "16.0.6",
    "electron-builder": "20.15.1",
    "electron-prebuilt-compile": "2.0.2",
    "typescript": "2.9.2"
  },
  "build": {
    "productName": "Reverse Image Finder",
    "publish": {
      "provider": "github",
      "owner": "Glinkis",
      "repo": "reverse-image-finder",
      "private": false
    },
    "dmg": {},
    "win": {
      "target": [
        "nsis",
        "zip"
      ]
    },
    "nsis": {
      "oneClick": false,
      "allowToChangeInstallationDirectory": true
    },
    "directories": {
      "output": "release"
    }
  }
}