{
  "name": "reverse-image-finder",
  "version": "0.4.0",
  "description": "A reverse image finder application.",
  "main": "start.js",
  "scripts": {
    "postinstall": "electron-builder install-app-deps",
    "serve": "electron .",
    "build": "webpack --mode development --progress",
    "start": "npm-run-all --parallel build serve",
    "pack": "electron-builder --dir",
    "dist": "electron-builder",
    "test": "electron-mocha -r ts-node/register test/**/*.test.ts --renderer",
    "test:i": "electron-mocha -r ts-node/register test/**/*.test.ts --interactive"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Glinkis/reverse-image-finder.git"
  },
  "author": "Victor Glind√•s",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/Glinkis/reverse-image-finder/issues"
  },
  "homepage": "https://github.com/Glinkis/reverse-image-finder#readme",
  "dependencies": {
    "coffee-script": "~ 1.7.1",
    "coffeescript-module": "~ 0.2.1",
    "electron-devtools-installer": "2.2.4",
    "electron-reload": "1.2.5",
    "electron-squirrel-startup": "1.0.0",
    "electron-updater": "3.0.3",
    "iconv-lite": "~ 0.4.4",
    "jspack": "~ 0.0.3",
    "lodash": "~ 2.4",
    "mobx": "5.0.3",
    "mobx-react": "5.2.3",
    "parse-engine-data": "~ 0.1",
    "pdfjs-dist": "2.0.489",
    "pixelmatch": "4.0.2",
    "react": "16.4.2",
    "react-dom": "16.4.2",
    "react-hot-loader": "4.3.4",
    "rsvp": "~ 3.0.6",
    "sharp": "0.20.5"
  },
  "devDependencies": {
    "@types/chai": "4.1.4",
    "@types/electron": "1.6.10",
    "@types/html-webpack-plugin": "3.2.0",
    "@types/mocha": "5.2.5",
    "@types/pdfjs-dist": "0.1.2",
    "@types/pixelmatch": "4.0.0",
    "@types/react": "16.4.7",
    "@types/react-dom": "16.0.6",
    "@types/sharp": "0.17.9",
    "@types/webpack-env": "1.13.6",
    "chai": "4.1.2",
    "coffee-loader": "0.9.0",
    "coffeescript": "1.12.7",
    "css-loader": "1.0.0",
    "electron": "2.0.6",
    "electron-builder": "20.19.1",
    "electron-mocha": "6.0.4",
    "html-webpack-plugin": "3.2.0",
    "mocha": "5.2.0",
    "node-loader": "0.6.0",
    "node-sass": "4.9.2",
    "npm-run-all": "4.1.3",
    "sass-loader": "7.1.0",
    "style-loader": "0.21.0",
    "ts-loader": "4.4.2",
    "ts-node": "7.0.0",
    "typescript": "3.0.1",
    "webpack": "4.16.5",
    "webpack-cli": "3.1.0"
  },
  "build": {
    "productName": "Reverse Image Finder",
    "files": [
      "!test/*"
    ],
    "publish": {
      "provider": "github",
      "owner": "Glinkis",
      "repo": "reverse-image-finder",
      "private": false
    },
    "dmg": {},
    "win": {
      "target": [
        "nsis",
        "zip"
      ]
    },
    "nsis": {
      "oneClick": false,
      "allowToChangeInstallationDirectory": true
    },
    "directories": {
      "output": "release"
    }
  }
}
