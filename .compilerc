{
  "env": {
    "development": {
      "text/typescript":{
        "target": "es6",
        "module": "commonjs",
        "sourceMap": true,
        "inlineSourceMap": true,
        "experimentalDecorators": true,
        "allowJs": true,
        "jsx": "react",
        "strict": true,
        "lib": ["dom"]
      }
    },
    "production": {
      "text/typescript": {
        "target": "es6",
        "module": "commonjs",
        "sourceMap": false,
        "experimentalDecorators": true,
        "allowJs": true,
        "jsx": "react",
        "lib": ["dom"]
      }
    }
  }
}